generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "linux-musl"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Medications {
  id           String   @id @default(uuid()) @db.Uuid
  childId      String  
  name         String
  dosage       String
  frequency    String
  description  String?
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}
