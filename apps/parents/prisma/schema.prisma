generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "linux-musl"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Parents {
  id           String   @id @default(uuid()) @db.Uuid
  accountId    String   @unique
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}

model ParentClinic {
  id          String
  parentId    String
  
  @@id ([id, parentId])
  @@unique([id, parentId])
}
