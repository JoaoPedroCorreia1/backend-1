generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "linux-musl"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Payments {
  id                  String   @id @unique
  userId              String
  providerId          String?
  type                String
  status              String
  amount              Decimal
  createdAt           DateTime @default(now())
  updatedAt           DateTime @updatedAt
}
