generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "linux-musl"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Accounts {
  id                   String   @id @default(uuid()) @db.Uuid
  subscriptionPlanId   String?  
  email                String   @unique
  name                 String
  phone                String
  password             String
  userType             String
  createdAt            DateTime @default(now())
  updatedAt            DateTime @updatedAt
}
