generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "linux-musl"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Clinics {
  id           String   @id @default(uuid()) @db.Uuid
  accountId    String?  @unique
  cnpj         String?
  name         String
  description  String
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}
